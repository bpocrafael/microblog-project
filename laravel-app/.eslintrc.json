{
    "env": {
        "browser": true,
        "es2022": true
    },
    "extends": [
        "plugin:prettier/recommended",
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "prettier",
        "vue"
    ],
    "rules": {
        "no-console": 2
    }
}
